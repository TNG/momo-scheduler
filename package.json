{
  "name": "@tngtech/momo-scheduler",
  "version": "3.0.0",
  "description": "momo is a scheduler that persists jobs in mongodb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TNG/momo-scheduler.git"
  },
  "keywords": [
    "delayed",
    "interval",
    "job",
    "jobs",
    "runner",
    "scheduler",
    "task",
    "recurrent"
  ],
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && tsc",
    "format": "biome check --write",
    "lint": "biome ci",
    "clean": "npx rimraf dist",
    "test": "jest --detectOpenHandles",
    "test:unit": "npm run test \"^((?!\\.integration\\.).)*$\"",
    "test:integration": "npm run test integration"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "cron": "4.4.0",
    "cron-parser": "5.4.0",
    "human-interval": "2.0.1",
    "lodash": "4.17.21",
    "luxon": "3.7.2",
    "neverthrow": "8.2.0",
    "typed-emitter": "2.1.0",
    "uuid": "13.0.0"
  },
  "peerDependencies": {
    "mongodb": "5 || 6 || 7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@sinonjs/fake-timers": "14.0.0",
    "@types/human-interval": "1.0.2",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.23",
    "@types/luxon": "3.7.1",
    "@types/node": "22.19.7",
    "@types/sinonjs__fake-timers": "8.1.5",
    "@types/uuid": "11.0.0",
    "jest": "29.7.0",
    "mongodb-memory-server": "10.4.3",
    "pino": "9.14.0",
    "ts-jest": "29.4.6",
    "ts-mockito": "2.6.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  }
}
