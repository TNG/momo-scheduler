{
  "name": "@tngtech/momo-scheduler",
  "version": "1.1.0",
  "description": "momo is a scheduler that persists jobs in mongodb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TNG/momo-scheduler.git"
  },
  "keywords": [
    "delayed",
    "interval",
    "job",
    "jobs",
    "runner",
    "scheduler",
    "task",
    "recurrent"
  ],
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "npx rimraf dist",
    "test": "jest",
    "test:unit": "npm run test \"^((?!\\.integration\\.).)*$\"",
    "test:integration": "npm run test integration",
    "lint": "prettier --check \"**/*.ts\"  && eslint --ext .ts,.js,.json .",
    "format": "prettier --write \"**/*.ts\" && eslint --fix --ext .ts,.js,.json ."
  },
  "dependencies": {
    "cron": "2.1.0",
    "cron-parser": "4.6.0",
    "human-interval": "2.0.1",
    "lodash": "4.17.21",
    "luxon": "3.0.4",
    "mongodb": "4.10.0",
    "neverthrow": "5.0.1",
    "typed-emitter": "2.1.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "9.1.2",
    "@types/cron": "2.0.0",
    "@types/human-interval": "1.0.0",
    "@types/jest": "28.1.8",
    "@types/lodash": "4.14.186",
    "@types/luxon": "3.0.2",
    "@types/node": "18.11.7",
    "@types/pino": "7.0.4",
    "@types/sinonjs__fake-timers": "8.1.2",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-jsdoc": "39.3.13",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "mongodb-memory-server": "8.9.3",
    "pino": "8.6.1",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-mockito": "2.6.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
