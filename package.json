{
  "name": "@tngtech/momo-scheduler",
  "version": "2.0.1",
  "description": "momo is a scheduler that persists jobs in mongodb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TNG/momo-scheduler.git"
  },
  "keywords": [
    "delayed",
    "interval",
    "job",
    "jobs",
    "runner",
    "scheduler",
    "task",
    "recurrent"
  ],
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "npx rimraf dist",
    "test": "jest --detectOpenHandles",
    "test:unit": "npm run test \"^((?!\\.integration\\.).)*$\"",
    "test:integration": "npm run test integration",
    "lint": "prettier --check \"**/*.ts\"  && eslint --ext .ts,.js,.json .",
    "format": "prettier --write \"**/*.ts\" && eslint --fix --ext .ts,.js,.json ."
  },
  "dependencies": {
    "cron": "2.4.0",
    "cron-parser": "4.8.1",
    "human-interval": "2.0.1",
    "lodash": "4.17.21",
    "luxon": "3.3.0",
    "neverthrow": "6.0.0",
    "typed-emitter": "2.1.0",
    "uuid": "9.0.0"
  },
  "peerDependencies": {
    "mongodb": "4 || 5"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "10.3.0",
    "@types/cron": "2.0.1",
    "@types/human-interval": "1.0.0",
    "@types/jest": "29.5.3",
    "@types/lodash": "4.14.195",
    "@types/luxon": "3.3.1",
    "@types/node": "16.18.38",
    "@types/pino": "7.0.4",
    "@types/sinonjs__fake-timers": "8.1.2",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jsdoc": "46.4.5",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.2",
    "mongodb-memory-server": "8.13.0",
    "pino": "8.14.2",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "ts-mockito": "2.6.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
