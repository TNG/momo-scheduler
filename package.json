{
  "name": "@tngtech/momo-scheduler",
  "version": "2.4.0",
  "description": "momo is a scheduler that persists jobs in mongodb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TNG/momo-scheduler.git"
  },
  "keywords": [
    "delayed",
    "interval",
    "job",
    "jobs",
    "runner",
    "scheduler",
    "task",
    "recurrent"
  ],
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "npx rimraf dist",
    "test": "jest --detectOpenHandles",
    "test:unit": "npm run test \"^((?!\\.integration\\.).)*$\"",
    "test:integration": "npm run test integration",
    "lint": "prettier --check \"**/*.ts\"  && eslint",
    "format": "prettier --write \"**/*.ts\" && eslint --fix"
  },
  "dependencies": {
    "cron": "4.3.1",
    "cron-parser": "5.2.0",
    "human-interval": "2.0.1",
    "lodash": "4.17.21",
    "luxon": "3.6.1",
    "neverthrow": "8.2.0",
    "typed-emitter": "2.1.0",
    "uuid": "11.1.0"
  },
  "peerDependencies": {
    "mongodb": "4 || 5 || 6"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "14.0.0",
    "@stylistic/eslint-plugin": "4.4.1",
    "@types/human-interval": "1.0.2",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.18",
    "@types/luxon": "3.6.2",
    "@types/node": "22.15.32",
    "@types/sinonjs__fake-timers": "8.1.5",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.33.1",
    "@typescript-eslint/parser": "8.33.1",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-jsdoc": "51.0.3",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.4.1",
    "jest": "29.7.0",
    "mongodb-memory-server": "10.1.4",
    "pino": "9.7.0",
    "prettier": "3.5.3",
    "ts-jest": "29.4.0",
    "ts-mockito": "2.6.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  }
}
