{
  "name": "@tngtech/momo-scheduler",
  "version": "2.1.0",
  "description": "momo is a scheduler that persists jobs in mongodb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TNG/momo-scheduler.git"
  },
  "keywords": [
    "delayed",
    "interval",
    "job",
    "jobs",
    "runner",
    "scheduler",
    "task",
    "recurrent"
  ],
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "npx rimraf dist",
    "test": "jest --detectOpenHandles",
    "test:unit": "npm run test \"^((?!\\.integration\\.).)*$\"",
    "test:integration": "npm run test integration",
    "lint": "prettier --check \"**/*.ts\"  && eslint --ext .ts,.js,.json .",
    "format": "prettier --write \"**/*.ts\" && eslint --fix --ext .ts,.js,.json ."
  },
  "dependencies": {
    "cron": "3.1.7",
    "cron-parser": "4.9.0",
    "human-interval": "2.0.1",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "neverthrow": "6.2.2",
    "typed-emitter": "2.1.0",
    "uuid": "10.0.0"
  },
  "peerDependencies": {
    "mongodb": "4 || 5 || 6"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "11.2.2",
    "@types/human-interval": "1.0.2",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.6",
    "@types/luxon": "3.4.2",
    "@types/node": "20.14.9",
    "@types/sinonjs__fake-timers": "8.1.5",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsdoc": "48.5.2",
    "eslint-plugin-markdown": "4.0.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "mongodb-memory-server": "9.4.0",
    "pino": "9.2.0",
    "prettier": "3.3.2",
    "ts-jest": "29.2.0",
    "ts-mockito": "2.6.1",
    "ts-node": "10.9.2",
    "typescript": "5.5.3"
  }
}
