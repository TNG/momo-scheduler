{
  "name": "@tngtech/momo-scheduler",
  "version": "0.2.0",
  "description": "momo is a scheduler that persists jobs in mongodb",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TNG/momo-scheduler.git"
  },
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "npx rimraf dist",
    "test": "jest --detectOpenHandles --forceExit",
    "test:unit": "npm run test \"^((?!\\.integration\\.).)*$\"",
    "test:integration": "npm run test integration",
    "lint": "prettier --check \"**/*.ts\"  && eslint \"**/*.{ts,js,json}\"",
    "format": "prettier --write \"**/*.ts\" && eslint \"**/*.{ts,js,json}\" --fix"
  },
  "dependencies": {
    "human-interval": "2.0.1",
    "lodash": "4.17.21",
    "luxon": "2.0.1",
    "mongodb": "3.6.10",
    "typed-emitter": "1.3.1",
    "typeorm": "0.2.28",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "7.1.2",
    "@types/human-interval": "1.0.0",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.171",
    "@types/luxon": "1.27.1",
    "@types/mongodb": "3.6.20",
    "@types/node": "16.3.2",
    "@types/pino": "6.3.9",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsdoc": "35.4.3",
    "eslint-plugin-markdown": "2.2.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "27.0.6",
    "mongodb-memory-server": "7.2.1",
    "pino": "6.12.0",
    "prettier": "2.3.2",
    "ts-jest": "27.0.3",
    "ts-mockito": "2.6.1",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "uuid": "8.3.2"
  }
